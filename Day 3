2:
var num =prompt("Enter You Marks");
if (80<=num && num<=100) {
    console.log("A")
} 
else if(70<=num && num<=79){
    console.log("b")
}
else if(60<=num && num<=69){
    console.log("C")
}
else if(50<=num && num<=59){
console.log("D")
}
else if(0<=num && num<=49){
    console.log("F")
}

else{
console.log("Plese Enter valid Marks")
}
3:
/No is +ve OR -ve/
let num=prompt("Enter a number")
if(num<0){
    console.log("Your no is Negtive")
}
else{
    console.log("Your no is postive")
}
4:
/Vote age/
let age=prompt("Enter your age");
if(age<18){
    console.log("You Can't vote");
}
else{
    console.log("You Can Vote");
}
5:
/Days using Nos/
let num=prompt("Enter Day No B/W 1-7")
if(num==1){
    console.log("Sunday");
}
else if(num==2){
    console.log("Monday");
}
else if(num==3){
    console.log("Tuesday");
}
else if(num==4){
    console.log("Wednesday");
}
else if(num==5){
    console.log("Thursday");
}
else if(num==6){
    console.log("Friday");
}
else if(num==7){
    console.log("Saturday");
}
else{
    console.log("Enter a valid No b/w 1-7 ");
}
6:
/Leap Year/
let year=prompt("Enter year");
let res1=year%4;
let res2=year%400;
if(res1==0 && res2==0 ){
    console.log("Yes it is a leap year");
}
else{
    console.log("Not it is not a leap year");
}
7:
/Check Even or Odd Nos/
let num=prompt("Enter a Number")
let res=num%2;
if(res==0){
    console.log("This is a Even")
}
else{
    console.log("This is a ODD")
}
8:Check Largest no b/w 3 nos
let a=prompt("Please Enter First Value");
let b=prompt("Please Enter Seconed Value");
let c=prompt("Please Enter Third Value");
if(b<a && c<a){
    console.log("your given No" + a + "is grater than other numbers");
}
else if(a<b && c<b){
    console.log("your given No" + b + "is grater than other numbers");
}
else if(b<c && a<c){
    console.log("your given No" + c +  "is grater than other numbers");
}
else {
console.log("enter valid nos")
}
9:
/Vowel OR Constant/
var alpha=prompt("Enter an alphabat");
if("a"==alpha || alpha=="A"){
    console.log("This is a vowel");
}
else if("e"==alpha || alpha=="E"){
    console.log("This is a vowel");
}
else if("i"==alpha || alpha=="I"){
    console.log("This is a vowel");
} 
else if("o"==alpha || alpha=="O"){
    console.log("This is a vowel");
}
else if("u"==alpha || alpha=="U"){
    console.log("This is a vowel");
}
else{
    console.log("This is a Constant");
}
